{% extends 'builder/blocks/base.twig' %}
{% block block_content %}
    <div class="row">
    {% for image in item.Images %}
        <div class="col-12 col-md-6 col-lg-4">
            <img src="{{ Image(image.Image).src }}" alt="{{ Image(image.Image).alt }}" title="{{ Image(image.Image).title }}"/>
        </div>
    {% endfor %}
    </div>
{% endblock %}